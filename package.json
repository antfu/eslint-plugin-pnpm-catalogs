{
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "pnpm -r run build",
    "stub": "pnpm -r run stub",
    "lint": "eslint .",
    "release": "bumpp -r && pnpm -r publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@antfu/ni": "catalog:",
    "@antfu/utils": "catalog:",
    "@types/js-yaml": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "eslint-vitest-rule-tester": "catalog:",
    "jiti": "catalog:",
    "js-yaml": "catalog:",
    "jsonc-eslint-parser": "catalog:",
    "lint-staged": "catalog:",
    "simple-git-hooks": "catalog:",
    "tinyexec": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vitest-package-exports": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
